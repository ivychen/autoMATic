int main() {
	int i;
	int j;

	i = 0;
	j = 5;
	i = i + j;
	j = i + j;

	println(i); // 5
	println(j); // 10
	println(j - 1); // 9
	println((j - 1) * 5); // 45

	j = j - 5; // 5
	if (i == j) println("i == j"); // true

	i = i - 1; // 4

	if (i == j) println("i == j"); // false
	if (i != j) println("i != j"); // true
	if (i < j) println("i < j"); // true
	if (i <= j) println("i <= j"); // true
	if (j <= j) println("j <= j"); // true

	if (j <= i) println("j <= i"); // false
	if (j > i) println("j > i"); // true
	if (j >= i) println("j >= i"); // true
	if (j >= j) println("j >= j"); // true

        i = 8;
        j = 2;
        println(i / j); // 4
        println(i / (j * 2)); // 2
        println(j ** 3); // 8

	return 0;
}
