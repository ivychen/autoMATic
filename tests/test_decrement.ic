int main() {
	int i;
	int j;

	i = 5;
	j = i;

	println(i); // 5
  println(j); // 5

  i--;
  j--;

  j = j - 1;
  if (i-- == j) println("i-- == j"); // true
  j = j - 1;
  if (i-- == j) println("i-- == j"); // true
  j = j - 1;
  if (i-- == j) println("i-- == j"); // true

  println(i); // 1
  println(j); // 1

	return 0;
}
