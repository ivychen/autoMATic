int main() {
	float x;
	float y;
	float z;
	float w;

	x = 0.0;
	y = 1.0;
	if (y == 1.0) {
		printstr("y == 1");
	}

	x = x + y;
	printflt(x); // 1.0

	x = -x;
	printflt(x); // -1.0

	z = x / 2.0;
	printflt(z); // -0.5

	w = x * 5.0 - (2.0 / 2.0);
	printflt(w); // -6.0

	if (w < z) printstr("w < z");
	if (w > z) printstr("w > z");

	if (w >= z) printstr("w >= z");
	if (z >= w) printstr("z >= w");

	if (z <= w) printstr("z <= w");
	if (w <= z) printstr("w <= z");

	if (z <= z) printstr("z <= z");

	if (w == z) printstr("w == z");
	if (z == z) printstr("z == z");
	if (w == w) printstr("w == w");
	if (w != z) printstr("w != z");

	return 0;
}
