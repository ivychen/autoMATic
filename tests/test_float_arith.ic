int main() {
	float x;
	float y;
	float z;
	float w;

	x = 0.0;
	y = 1.0;
	if (y == 1.0) {
		print("y == 1");
	}

	x = x + y;
	print(x); // 1.0

	x = -x;
	print(x); // -1.0

	z = x / 2.0;
	print(z); // -0.5

	w = x * 5.0 - (2.0 / 2.0);
	print(w); // -6.0

	if (w < z) print("w < z");
	if (w > z) print("w > z");

	if (w >= z) print("w >= z");
	if (z >= w) print("z >= w");

	if (z <= w) print("z <= w");
	if (w <= z) print("w <= z");

	if (z <= z) print("z <= z");

	if (w == z) print("w == z");
	if (z == z) print("z == z");
	if (w == w) print("w == w");
	if (w != z) print("w != z");

        x = 2.0;
        print(x ** 2); // 4.0
        print(x ** 3.0); // 8.0
        print(x ** 4.0); // 16.0

	return 0;
}
