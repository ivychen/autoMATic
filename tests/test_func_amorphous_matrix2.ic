int matrix test(int matrix a) {
    int i, j;
    int matrix m;

    i = rows(a);
    j = cols(a);
    m = imat(i, j);
    m[i - 1][j - 1] = 0;

    for (i = 0; i < rows(a); i++) {
        for (j = 0; j < cols(a); j++) {
            m[i][j] = a[i][j] + 5;
        }
    }

    return m;
}

int main() {
    int matrix x = [ [2,2]; [5,5] ];
    int matrix y = test(x);

    print(y[0][0]); // 7
    print(y[0][1]); // 7
    print(y[1][0]); // 10
    print(y[1][1]); // 10

    return 0;
}
