int main() {
	int i;
	int j;

	i = 0;
	j = i;

	println(i); // 0
  println(j); // 0

  i++;
  j++;

  j = j + 1;
  if (i++ == j) println("i++ == j"); // true
  j = j + 1;
  if (i++ == j) println("i++ == j"); // true
  j = j + 1;
  if (i++ == j) println("i++ == j"); // true

  println(i); // 4
  println(j); // 4

	return 0;
}
