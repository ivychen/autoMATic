int main() {
    float matrix x = [[ 4.3, 5.1, 6.2 ]; [1.0, 0.0, 491.5]; [6.5, 3.0, 3.5]];
    float matrix y = x';
    int i;
    int j;

    for (i = 0; i < rows(x); i++) {
        for (j = 0; j < rows(x); j++) {
            if (x[i][j] != y[j][i]) {
                println("failure");
                println(i);
                println(j);

                return 1;
            }
        }
    }

    println("success");
