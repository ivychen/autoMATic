int main() {
    int x = 4;
    string y = "test";

    test1(x, "test");
    return 0;
}

// main declarations of "x" and "y" should not conflict
void test1(int x, string y) {
    print(x);
    print(y);
    test2(x);
}

// test2's formal "x" should not conflict with test1's
void test2(int x) {
    print(x);
}
