int main() {
    bool matrix [3][3] x = [[ true, false, true ]; [ true, false, false ]; [ false, false, true ]];
    bool matrix [3][3] y = x';
    int i;
    int j;

    for (i = 0; i < rows(x); i++) {
        for (j = 0; j < rows(x); j++) {
            if (x[i][j] != y[j][i]) {
                println("failure");
                println(i);
                println(j);

                return 1;
            }
        }
    }

    println("success");
    return 0;
}
