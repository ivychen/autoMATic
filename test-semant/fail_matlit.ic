/* Create a matrix */
// Assign to matrix literal

matrix mat(bool is_square) {
	matrix M;

	if (is_square == true) {
    // FAIL: jagged matrix
		M = [[1,2,3];[3,2,1];[3,3]];
	} else {
		// 1D matrix
		M = [[1,2,3]];
	}
	return M;
}

int main() {
    bool x;
    x = true;

    mat(x); // returns square matrix

    x = false;
    mat(x); // returns 1D matrix
}
