/* Create a matrix */
// Assign to matrix literal

int matrix mat(bool is_square) {
	int matrix[3][3] M;
	int matrix[1][3] N;

	if (is_square == true) {
		M = [[1,2,3];[3,2,1];[3,3,3]];
		return M;
	} else {
		// 1D matrix
		N = [[1,2,3]];
		return N;
	}
}

int main() {
    bool x;
    x = true;

    mat(x); // returns square matrix

    x = false;
    mat(x); // returns 1D matrix
}
