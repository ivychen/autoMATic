/* additional printing functions */

// simple, unaligned print
void printimat(int matrix A) {
	int r = rows(A);
	int c = cols(A);

	print("[");
	print(r);
	print(" x ");
	print(c);
	println("]");

	for (int i = 0; i < r; i++) {
		for (int j = 0; i < c; j++) {
			print(A[i][j]);
			print(", ");
		}
		println("");	
	}
}
