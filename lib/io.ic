/* additional printing functions */

void printimat(int matrix A) {
	int r = rows(A);
	int c = cols(A);

	// print the dimensions
	print("[");
	print(r);
	print(" x ");
	print(c);
	println("]");

	// print the values
	int i;
	int j;
	for (i = 0; i < r; i++) {
		for (j = 0; i < c; j++) {
			print(A[i][j]);
			print(", ");
		}
		println("");	
	}
}
